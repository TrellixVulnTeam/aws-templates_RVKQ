{
    "LoggingEnabled": {
      "TargetBucket": "MyBucket",
      "TargetPrefix": "MyBucketLogs/",
      "TargetGrants": [
        {
          "Grantee": {
            "Type": "AmazonCustomerByEmail",
            "EmailAddress": "user@example.com"
          },
          "Permission": "FULL_CONTROL"
        },
        {
          "Grantee": {
            "Type": "Group",
            "URI": "http://acs.amazonaws.com/groups/global/AllUsers"
          },
          "Permission": "READ"
        }
      ]
    }
  }